<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" href="{{ asset('stylesheets/default/pages/home.css') }}" />
            <link rel="stylesheet" href="{{ asset('bootstrap/css/bootstrap.css') }}"/>
            <link rel="stylesheet" href="{{ asset('node_modules/owl.carousel/dist/assets/owl.carousel.css') }}"/>
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        {% block body %}{% endblock %}
        {% block javascripts %}
            <script src="{{ asset('node_modules/jquery/dist/jquery.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('js/default/home.js') }}" type="text/javascript"></script>
        {% endblock %}
    </body>
</html>
